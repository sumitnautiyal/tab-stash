{
  "manifest_version": 3,
  "name": "Tab Stash",
  "version": "1.8",
  "description": "Serialize browser state to Markdown and free memory.",
  "permissions": [
    "tabs",
    "clipboardWrite"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "tab-stash@thecsjourney.com",
      "strict_min_version": "142.0",
      "data_collection_permissions": {
        "interaction_data": false,
        "technical_data": false,
        "required": []
      }
    }
  },
  "icons": {
    "32": "icons/icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  }
}