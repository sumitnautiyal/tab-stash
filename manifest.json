{
  "manifest_version": 3,
  "name": "Tab Stash",
  "version": "1.5",
  "description": "Serialize browser state to Markdown and free memory.",
  "permissions": [
    "tabs",
    "clipboardWrite"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "tab-stash@thecsjourney.com",
      "strict_min_version": "109.0"
    }
  },
  "data_collection_permissions": {
    "interaction_data": false,
    "technical_data": false
  },
  "icons": {
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "96": "icons/icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  }
}